<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="900.0" prefWidth="1400.0" spacing="15.0" styleClass="root" stylesheets="@MainApp.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Uboat.component.main.UboatMainController">
   <children>
      <HBox fx:id="buttonsHbox" alignment="CENTER" prefHeight="88.0" prefWidth="1398.0" spacing="10.0" visible="false">
         <children>
            <Button fx:id="machineButton" mnemonicParsing="false" onAction="#machineButtonClicked" prefHeight="50.0" prefWidth="180.0" text="Machine" />
            <Button fx:id="ContestButton" mnemonicParsing="false" onAction="#ContestButtonClicked" prefHeight="50.0" prefWidth="180.0" text="Contest" />
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
      </HBox>
    <AnchorPane fx:id="mainPanel" minHeight="0.0" minWidth="0.0" prefHeight="800.0" prefWidth="1400.0" />
   </children>
</VBox>
