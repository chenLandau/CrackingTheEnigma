<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="348.0" prefWidth="802.0" stylesheets="@../../cssPackage/TableView.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Allies.component.ContestScreen.AgentsProgressData.AgentsProgressController">
   <children>
      <Label id="smallTitle" alignment="CENTER" layoutY="14.0" prefHeight="53.0" prefWidth="806.0" text="Agents Progress details:" textFill="#bfbebe">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font></Label>
      <TableView fx:id="agentProgressListTableView" layoutX="5.0" layoutY="67.0" prefHeight="239.0" prefWidth="798.0">
         <columns>
            <TableColumn fx:id="agentName" prefWidth="119.0" styleClass="column-header-background" text="Agent name" />
            <TableColumn fx:id="missionsOnQueue" prefWidth="185.0" styleClass="column-header-background" text="Missions left" />
            <TableColumn fx:id="totalAmountOfMissionTaken" prefWidth="122.0" styleClass="column-header-background" text="Total missions" />
            <TableColumn fx:id="missionsCompleted" prefWidth="186.0" styleClass="column-header-background" text="Missions completed" />
            <TableColumn fx:id="totalAmountOfCandidatesFound" prefWidth="117.0" styleClass="column-header-background" text="Total candidates" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </TableView>
   </children>
</AnchorPane>
