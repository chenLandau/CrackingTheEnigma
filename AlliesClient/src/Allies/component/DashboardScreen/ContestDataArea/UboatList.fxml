<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="855.0" stylesheets="@../../cssPackage/TableView.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Allies.component.DashboardScreen.ContestDataArea.UboatListController">
   <children>
      <TableView fx:id="uboatListTableView" layoutX="6.0" layoutY="63.0" onMouseClicked="#getTableChoose" prefHeight="230.0" prefWidth="845.0">
         <columns>
            <TableColumn fx:id="battleFieldName" prefWidth="200.0" styleClass="column-header-background" text="Battle field name" />
            <TableColumn fx:id="uboatName" prefWidth="116.0" styleClass="column-header-background" text="Uboat name" />
            <TableColumn fx:id="gameStatus" prefWidth="135.0" styleClass="column-header-background" text="Game status" />
            <TableColumn fx:id="level" prefWidth="109.0" styleClass="column-header-background" text="Level" />
            <TableColumn fx:id="alliesAmount" prefWidth="160.0" styleClass="column-header-background" text="Allies amount" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </TableView>
      <Label id="smallTitle" alignment="CENTER" layoutX="2.0" prefHeight="55.0" prefWidth="855.0" text="Contests details:" textFill="#bfbebe">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font></Label>
   </children>
</AnchorPane>
